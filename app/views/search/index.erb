<h1><%= @stations.count %> Results</h1>
  <% @stations.each do |station| %>
  <ul class="station">
  <li class="name">Name: <%= station.name %></li>
  <li class="fuel_types">Fuel Types: <%= station.fuel_types %></li>
  <li class="address">Address: <%= station.address %></li>
  <li class="access_times">Access Times: <%= station.access_times %></li>
  <li class="distance">Distance in Miles: <%= station.distance %></li>
  </ul>
<% end %>
